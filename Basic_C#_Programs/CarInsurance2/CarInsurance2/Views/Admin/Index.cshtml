@using CarInsurance2.ViewModel

@model List<QuoteVM>


<table class="table">
    <tr>
        <th>
            First Name
        </th>
        <th>
            Last Name
        </th>
        <th>
            Email Address
        </th>
        <th>
            Quote
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.FirstName
            </td>
            <td>
                @item.LastName
            </td>
            <td>
                @item.EmailAddress
            </td>
            <td>
                @item.Quote
            </td>
        </tr>
    }

</table>
